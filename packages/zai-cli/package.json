  "version": "1.1.0",
  "description": "CLI for Z.AI capabilities: vision analysis, web search, web reader, and GitHub repo exploration",
  "type": "module",
  "bin": {
    "zai-cli": "./bin/zai-cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/numman-ali/zai-cli.git",
    "directory": "packages/zai-cli"
  },
